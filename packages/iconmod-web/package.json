    "jszip": "^3.10.1",
    "lru-cache": "^11.0.0",
    "svg-packer": "^0.0.3",
    "typescript": "^5.5.4",
    "unocss": "^0.62.2",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.1",
    "vite-plugin-pages": "^0.32.3",
    "vite-plugin-pwa": "^0.20.1",
    "vue-tsc": "^2.0.29"
  },
  "pnpm": {
    "neverBuiltDependencies": [
      "ttf2woff2",
      "vite-plugin-esmodule"
    ]
