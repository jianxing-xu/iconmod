    "dexie": "^4.0.8",
    "file-saver": "^2.0.5",
    "floating-vue": "^5.2.2",
    "fzf": "^0.5.2",
    "hotkeys-js": "^3.13.7",
    "iconify-icon": "^2.1.0",
    "universal-cookie": "^7.2.0",
    "vue": "^3.4.38",
    "vue-chemistry": "^0.2.2",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/fs-extra": "^11.0.4",
    "@vitejs/plugin-vue": "^5.1.2",
    "dayjs": "^1.11.12",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
