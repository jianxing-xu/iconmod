  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --port 5000",
    "build": "vite build --mode lib",
    "release": "npx bumpp && npm publish"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.19",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^4.2.4"
  }
