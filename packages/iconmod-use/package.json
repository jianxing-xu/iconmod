{
  "name": "@iconmod/use",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@9.9.0",
  "description": "iconmod icon use",
  "author": "jianxing-xu",
  "license": "MIT",
  "homepage": "https://github.com/jianxing-xu/iconmod/tree/main/packages/iconmod-use",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jianxing-xu/iconmod/"
  },
  "bugs": "https://github.com/jianxing-xu/iconmod/issues",
  "keywords": ["icon", "iconfont", "iconmod", "iconify", "icones", "svg-symbol"],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.umd.cjs",
